<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unknown.notyet">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
        <activity android:name=".Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DialogYesNo"/>
        <activity android:name=".DialogInputSaveCancel"/>
        <activity android:name=".Home"/>
        <activity android:name=".HatchEgg"/>
        <activity android:name=".LeveledUpScreen"/>
        <activity android:name=".Battle"/>
        <activity android:name=".Inventory"/>
        <activity android:name=".CreatureInfo"/>
        <activity android:name=".Store"/>
        <activity android:name=".Searcher"/>
    </application>

</manifest>